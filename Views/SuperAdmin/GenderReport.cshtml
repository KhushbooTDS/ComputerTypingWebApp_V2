@model List<Gender>
@{

    Layout = "~/Views/Shared/_SuperAdminLayout.cshtml";
}
<p class="h3">Gender Report</p>
<input class="btn btn-info btn-icon" style="width:200px;" type="button" value="Add Gender" onclick="location.href='@Url.Action("AddGender","SuperAdmin", new{dashId = 1})'" />
<hr />

<div class="container">
    <div class="card">
        <h5 class="card-header">Gender Report</h5>
        <div class="card-body">
            <div class="table-responsive text-nowrap">
                <table class="table table-bordered">
                    <tr>
                        <th>
                            Gender
                        </th>
                    </tr>

                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.Name)
                            </td>
                        </tr>
                    }
                </table>
            </div>
        </div>
    </div>

</div>