@model StudentGrowthDetails

@{
    ViewData["Title"] = "REPORTINGSTUDENTGROWTH";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h6 style="color:indianred">SESSION: @ViewBag.startsession To  @ViewBag.endsession </h6>

<div class="row col-lg-12">
    <div class="form-group">
        <h5 class="row col-12 card py-2" style="background-color: wheat; text-align: center;">
            SHOWING DETAILS OF STUDENT GROWTH
        </h5>
    </div>
</div>

@foreach (var subject in Model.SubjectList)
{
<div class="row col-lg-12">
    <div class="form-group">
        <h5 class="row col-8 card py-2" style="background-color: blue; color:white; text-align: left;">
            @subject.SubjectName
        </h5>
    </div>
</div>
}
<div class="row">
    @foreach (var practice in Model.PracticeList)
        {
    <div class="col-4">
        <div class="card">
            <div class="card-body">
                <h5 class="card-title">@practice.PracticName</h5>
                    <a title="View Growth Details" asp-action="ViewPracticesDetails" asp-controller="Admin" asp-route-id="@practice.PracticeId">
                    View Accuracy Details
                </a>
            </div>
        </div>
    </div>
        }
</div>
