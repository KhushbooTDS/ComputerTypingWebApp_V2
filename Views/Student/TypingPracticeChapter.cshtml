@model ComputerTypingWebApp.Models.CoursesUpload

@{
    Layout = null;
}
<link href="~/css/typingpracticechapter/bootstrap.min.css" rel="stylesheet" />
<link href="~/css/typingpracticechapter/style.css" rel="stylesheet" />
<link href="~/css/typingpracticechapter/gesture.css" rel="stylesheet" />
<div class="container-fluid typingBody">
    <input type="hidden" value="@Model.SubjectId" id="SubjectId" />
    <input type="hidden" value="@Model.PracticeId" id="PracticeId" />
    <div id="timer" class="mt-3 p-1"></div>
    <div class="" id="typingParagraphHolder">
        <audio id="error" src="~/sound/typingpracticechapter/error.mp3" preload="auto" type="audio/mpeg" hidden></audio>
        <audio id="keypress" src="~/sound/typingpracticechapter/keypress.mp3" preload="auto" type="audio/mpeg" hidden></audio>
        <div class="typingParagraph m-2 text-wrap" style="height:220px; overflow-y:scroll; overflow-x:hidden; word-spacing:-5px;">
            @if (Model.PracticeData != null)
            {
                var incr = 1;
                foreach (char item in Model.PracticeData)
                {
                    if (item == ' ')
                    {
                        <span class="px-2 ch c-@incr">@item</span>
                    }
                    else if (item == '"')
                    {
                        <span class="px-1 ch c-@incr">"</span>
                    }
                    else
                    {
                        <span class="ch c-@incr">@item</span>
                    }
                    incr++;
                }
            }

        </div>
    </div>
