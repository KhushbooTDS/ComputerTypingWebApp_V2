<div id="handGesture" class="row">
    <div class="col-6" id="leftHand"></div>
    <div class="col-6" id="rightHand"></div>
</div>
<div class="container-fluid resultBody d-none col-md-9 my-5 ">
    <div class="card ">
        <div class="card-header tex-center">
            <div class="row text-center my-1">
                <h5><span id="studentName">Ravi</span></h5>
            </div>
        </div>
        <div class="card-body">
            <div class="text-center mt-2 mb-5">
                <div class="d-flex justify-content-center">
                    <div class="card resultCard  mx-1">
                        <div class="card-header p-1">
                            <div id="">Gross Speed (WPM)</div>
                        </div>
                        <div class="card-body">
                            <div id="grossSpeed">55</div>
                        </div>
                    </div>
                    <div class="card resultCard mx-1">
                        <div class="card-header p-1">
                            <div id="">Net Speed (WPM)</div>
                        </div>
                        <div class="card-body">
                            <div id="netSpeed">50</div>
                        </div>
                    </div>
                    <div class="card resultCard mx-1">
                        <div class="card-header p-1">
                            <div id="">Correct Key</div>
                        </div>
                        <div class="card-body">
                            <div id="correctWord">100</div>
                        </div>
                    </div>
                    <div class="card resultCard mx-1">
                        <div class="card-header p-1">
                            <div id="">Incorrect Key</div>
                        </div>
                        <div class="card-body">
                            <div id="incorrectWord">10</div>
                        </div>
                    </div>
                    <div class="card resultCard mx-1">
                        <div class="card-header p-1">
                            <div id="">Accuracy</div>
                        </div>
                        <div class="card-body">
                            <div id="accuracy">90%</div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
